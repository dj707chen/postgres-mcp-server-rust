{
  "mcpServers": {
    "postgres": {
      "command": "/path/to/postgres-mcp-server-rust/target/release/postgres_mcp_server",
      "env": {
        "DATABASE_URL": "postgresql://postgres:postgres@localhost:5432/testdb"
      }
    },
    "postgres-with-write": {
      "command": "/path/to/postgres-mcp-server-rust/target/release/postgres_mcp_server",
      "env": {
        "DATABASE_URL": "postgresql://postgres:postgres@localhost:5432/testdb",
        "DANGEROUSLY_ALLOW_WRITE_OPS": "true"
      }
    }
  }
}
